@article{kargarCorporateBondLiquidity2021,
  title = {Corporate {{Bond Liquidity}} during the {{COVID-19 Crisis}}},
  author = {Kargar, Mahyar and Lester, Benjamin and Lindsay, David and Liu, Shuo and Weill, Pierre-Olivier and Zúñiga, Diego},

@article{lagosLiquidityAssetMarkets2009,
  title = {Liquidity in {{Asset Markets With Search Frictions}}},
  author = {Lagos, Ricardo and Rocheteau, Guillaume},
  date = {2009},
  journaltitle = {Econometrica},
  volume = {77},
  number = {2},
  pages = {403--426},
  issn = {1468-0262},
  doi = {10.3982/ECTA7250},
  url = {https://onlinelibrary.wiley.com/doi/abs/10.3982/ECTA7250},
  urldate = {2020-11-08},
  abstract = {We develop a search-theoretic model of financial intermediation in an over-the-counter market and study how trading frictions affect the distribution of asset holdings and standard measures of liquidity. A distinctive feature of our theory is that it allows for unrestricted asset holdings, so market participants can accommodate trading frictions by adjusting their asset positions. We show that these individual responses of asset demands constitute a fundamental feature of illiquid markets: they are a key determinant of trade volume, bid–ask spreads, and trading delays—the dimensions of market liquidity that search-based theories seek to explain.},
  langid = {english},
  keywords = {Bid–ask spreads,liquidity,search,trade volume,trading delays},
  file = {C\:\\Users\\z3410121\\Zotero\\storage\\3LRCHN2D\\Econometrica.pdf;C\:\\Users\\z3410121\\Zotero\\storage\\N53R6VFR\\Econometrica_Proofs.pdf;C\:\\Users\\z3410121\\Zotero\\storage\\R76AMGHR\\Lagos and Rocheteau - 2009 - Liquidity in Asset Markets With Search Frictions.pdf;C\:\\Users\\z3410121\\Zotero\\storage\\6D5HMKC2\\ECTA7250.html}
}

